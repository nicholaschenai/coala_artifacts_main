    def min_moves_to_sort_books():
        import sys
        input = sys.stdin.read
        data = input().split()
    
        N = int(data[0])
        books = list(map(int, data[1:]))
    
        def binary_search_lis(lis, book):
            low, high = 0, len(lis)
            while low < high:
                mid = (low + high) // 2
                if lis[mid] < book:
                    low = mid + 1
                else:
                    high = mid
            return low
    
        lis = []
        for book in books:
            pos = binary_search_lis(lis, book)
            if pos == len(lis):
                lis.append(book)
            else:
                lis[pos] = book
    
        min_moves = N - len(lis)
        print(min_moves)

    min_moves_to_sort_books()
