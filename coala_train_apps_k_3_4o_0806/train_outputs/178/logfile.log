         if len(nums) == 0:
             return 0
         res = [nums[0]]
         def binarySearch(l,target):
             left , right = 0 , len(l)-1
             while left < right:
                 mid = (left + right)//2
                 if l[mid] >= target:
                     right = mid
                 else:
                     left = mid + 1
             return left
