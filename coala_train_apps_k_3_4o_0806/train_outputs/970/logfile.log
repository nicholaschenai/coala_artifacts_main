def process_test_cases():
    import sys
    input = sys.stdin.read
    data = input().split()
    
    index = 0
    T = int(data[index])
    index += 1
    
    results = []
    
    for _ in range(T):
        N = int(data[index])
        index += 1
        a = list(map(int, data[index:index + N]))
        index += N
        Q = int(data[index])
        index += 1
        
        a_set = set(a)  # Convert list to set for O(1) lookup
        for _ in range(Q):
            x = int(data[index])
            y = int(data[index + 1])
            index += 2
            
            sum_xy = x + y
            if sum_xy in a_set:
                results.append(-1)
            else:
                # Count how many a_i are greater than sum_xy
                count = 0
                for ai in a:
                    if ai > sum_xy:
                        count += 1
                results.append(count)
    
    for result in results:
        print(result)

process_test_cases()
```

