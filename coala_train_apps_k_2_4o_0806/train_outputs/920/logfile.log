def calculate_minimum_awkwardness(test_cases):
    results = []
    for s in test_cases:
        count_b = s.count('b')
        count_g = s.count('g')
        # The minimum awkwardness is the product of the number of boys and girls
        minimum_awkwardness = count_b * count_g
        results.append(minimum_awkwardness)
    return results

def main():
