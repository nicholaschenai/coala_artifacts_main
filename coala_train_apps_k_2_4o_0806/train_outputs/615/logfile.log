    def process_test_case(N, Q, bills, queries):
        # Step 1: Create a prefix sum array
        prefix_sum = [0] * (N + 1)
        for i in range(1, N + 1):
            prefix_sum[i] = prefix_sum[i - 1] + bills[i - 1]

        # Step 2: Process each query
        results = []
        for X, Y in queries:
            sum_between = prefix_sum[Y] - prefix_sum[X - 1]
            results.append(sum_between)

        return results

    def main():
        import sys
        input = sys.stdin.read
        data = input().split()
    
        index = 0
        T = int(data[index])
        index += 1
        results = []
    
        for _ in range(T):
            N = int(data[index])
            Q = int(data[index + 1])
            index += 2
        
            bills = list(map(int, data[index:index + N]))
            index += N
        
            queries = []
            for _ in range(Q):
                X = int(data[index])
                Y = int(data[index + 1])
                index += 2
                queries.append((X, Y))
        
            results.extend(process_test_case(N, Q, bills, queries))
    
        for result in results:
            print(result)

    if __name__ == "__main__":
        main()

