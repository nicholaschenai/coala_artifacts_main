function calculate(s):
    initialize stack as empty
    initialize result as 0
    initialize sign as 1
    initialize i as 0

    while i < length of s:
        if s[i] is a digit:
            initialize num as 0
            while i < length of s and s[i] is a digit:
                num = num * 10 + int(s[i])
                increment i
            decrement i
            result += sign * num
        else if s[i] is '+':
            sign = 1
        else if s[i] is '-':
            sign = -1
        else if s[i] is '(':
            push result onto stack
            push sign onto stack
            result = 0
            sign = 1
        else if s[i] is ')':
            pop sign from stack
            pop previous_result from stack
            result = previous_result + sign * result
        increment i

    return result
