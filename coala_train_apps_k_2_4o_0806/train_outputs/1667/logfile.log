        current = array[i]
        remainder = current % (n - i if direction == 'left' else i + 1)
        
        if remainder < 3:
            result.append(current)
            i += step
        else:
            if direction == 'left':
                subarray = array[i:i + remainder]
                i += remainder
            else:
                subarray = array[i - remainder + 1:i + 1]
                i -= remainder
            
            result.append(process_subarray(subarray, depth - 1, direction))
    
    return result

