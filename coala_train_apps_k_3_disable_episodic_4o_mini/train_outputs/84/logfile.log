    def count_exterminable_subarrays(array):
        count = 0
        n = len(array)
    
        for start in range(n):
            stack = []
            for end in range(start, n):
                if not stack or (stack and stack[-1] != array[end]):
                    stack.append(array[end])
                else:
                    stack.pop()
                if not stack:
                    count += 1
        return count

    def solve():
        import sys
        input = sys.stdin.read
        data = input().splitlines()
    
        q = int(data[0])  # number of queries
        index = 1
        results = []
    
        for _ in range(q):
            n = int(data[index])  # length of array
            index += 1
            array = list(map(int, data[index].split()))  # the array itself
            index += 1
        
            result = count_exterminable_subarrays(array)
            results.append(result)
    
        sys.stdout.write('\n'.join(map(str, results)) + '\n')

    solve()

