    def gcd(a, b):
        while b != 0:
            temp = b
            b = a % b
            a = temp
        return a

    def lcm(a, b):
        return (a * b) // gcd(a, b)

    def lcm_of_three(x, y, z):
        return lcm(lcm(x, y), z)

